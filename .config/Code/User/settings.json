{
    "jshint.enable": false,
    "editor.lineNumbers": "relative",
    "vim.cmdLineInitialColon": true,
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.leader": ",",
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.easymotionMarkerFontSize": "17",
    "vim.easymotionMarkerFontWeight": "bold",
    "editor.wrappingIndent": "indent",
    "workbench.editor.enablePreview": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-j>"],
            "commands": [
                {
                    "command": "workbench.action.nextEditor",
                }
            ]
        },
        {
            "before": ["<C-k>"],
            "commands": [
                {
                    "command": "workbench.action.previousEditor",
                }
            ]
        },
        {
            "before": ["<leader>", "q"],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor",
                }
            ]
        },
        {
            "before": ["<leader>", "a"],
            "commands": [
                "workbench.view.search"
            ]
        },
        {
            "before": ["<leader>", "space"],
            "commands": [
                ":nohlsearch"
            ]
        },
        {
            "before": ["<leader>", "Q"],
            "commands": [
                {
                    "command": "workbench.action.closeOtherEditors",
                }
            ]
        },
        {
            "before": ["<leader>", "W"],
            "commands": [
                {
                    "command": "workbench.action.reopenClosedEditor",
                }
            ]
        },
        {
            "before": ["<leader>", "f"],
            "after": ["<leader>", "<leader>", "s"]
        },
        {
            "before": ["<leader>", "s"],
            "after": ["<leader>", "<leader>", "2", "s"]
        },
        {
            "before": ["s"],
            "after": ["<leader>", "<leader>", "2", "s"]
        },
        {
            "before": ["<C-t>"],
            "commands": [
                {
                    "command": "workbench.action.showAllSymbols"
                }
            ]
        },
        {
            "before": ["g", "s"],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger"
                }
            ]
        },
        {
            "before": ["g", "d"],
            "commands": [
                {
                    "command": "editor.action.goToDeclaration"
                }
            ]
        },
        {
            "before": ["g", "a"],
            "commands": [
                {
                    "command": "references-view.find"
                }
            ]
        }
    ],
    "python.venvPath": "/home/nikos/virtenvs",
    "window.zoomLevel": 0,
    "breadcrumbs.enabled": true,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "go.toolsGopath": "/home/nikos/workspace/go",
    "go.gopath": "/home/nikos/workspace/go",
    "go.goroot": "/usr/lib/go-1.14",
}
